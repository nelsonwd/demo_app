<h1>Listing interpros</h1>

<table>
  <tr>
    <th>Accession</th>
    <th>Desc</th>
    <th>Gene ontology</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @interpros.each do |interpro| %>
  <tr>
    <td><%= interpro.accession %></td>
    <td><%= interpro.desc %></td>
    <td><%= interpro.gene_ontology_id %></td>
    <td><%= link_to 'Show', interpro %></td>
    <td><%= link_to 'Edit', edit_interpro_path(interpro) %></td>
    <td><%= link_to 'Destroy', interpro, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Interpro', new_interpro_path %>
