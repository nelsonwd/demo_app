<h1>Listing de_analyses</h1>

<table>
  <tr>
    <th>Analysis method</th>
    <th>Script name</th>
    <th>Experiment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @de_analyses.each do |de_analysis| %>
  <tr>
    <td><%= de_analysis.analysis_method %></td>
    <td><%= de_analysis.script_name %></td>
    <td><%= de_analysis.experiment_id %></td>
    <td><%= de_analysis.default_treatment_id %></td>
    <td><%= link_to 'Show', de_analysis %></td>
    <td><%= link_to 'Edit', edit_de_analysis_path(de_analysis) %></td>
    <td><%= link_to 'Destroy', de_analysis, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New DE Analysis', new_de_analysis_path %>
